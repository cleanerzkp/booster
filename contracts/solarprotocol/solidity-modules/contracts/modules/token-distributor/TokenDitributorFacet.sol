pragma solidity ^0.8.9;

import {ITokenDitributor} from "./ITokenDitributor.sol";
import {LibTokenDistributor} from "./LibTokenDistributor.sol";

contract TokenDitributorFacet is ITokenDitributor {
    /**
     * @inheritdoc ITokenDitributor
     */
    function getTokenDistributionStrategy(bytes32 strategyId)
        external
        view
        returns (ITokenDitributor.Strategy memory)
    {
        return LibTokenDistributor.get(strategyId);
    }
}
